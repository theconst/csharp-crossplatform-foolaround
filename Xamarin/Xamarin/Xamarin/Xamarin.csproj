  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Xamarin.Forms" Version="2.5.0.280555" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="CommonDivisorLibrary">
      <HintPath>..\..\..\CommonDivisorLibrary\CommonDivisorLibrary\bin\Debug\netstandard2.0\CommonDivisorLibrary.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="App.xaml.cs">
